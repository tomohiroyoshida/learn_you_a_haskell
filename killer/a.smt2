(declare-fun x () Int)
(declare-fun y () Int)
(declare-fun z () Int)
(assert
   (and
      (<= 1 x) (<= x 9)
      (<= 1 y) (<= y 9)
      (<= 1 z) (<= z 9)
      (not (= x y))
      (not (= x z))
      (not (= y z))
      (= (+ x y) 4)
      (= (+ x y) (+ z z))
   )
)
(check-sat)
(get-value (x y z))